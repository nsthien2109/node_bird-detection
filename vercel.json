{
  "version": 2,
  "builds": [
    {
      "src": "./dist/index.js",
      "use": "@vercel/node",
      "config": {
        "command": "npm run start"
      }
    }
  ],
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
