{
  "version": 2,
  "builds": [
    {
      "src": "builds/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["builds/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "builds/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
